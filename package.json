{
    "name": "vscode-inspect-repro",
    "displayName": "VSCode Inspect repro",
    "description": "Extension inspect issue.",
    "version": "1.0.0",
    "publisher": "kanadigtest",
    "author": {
        "name": "Karthik Nadig"
    },
    "license": "MIT",
    "homepage": "https://github.com/Karthiknadig/vscode-inspect-repro",
    "repository": {
        "type": "git",
        "url": "https://github.com/Karthiknadig/vscode-inspect-repro"
    },
    "bugs": {
        "url": "https://github.com/Karthiknadig/vscode-inspect-repro/issues"
    },
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#1e415e",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.62.0"
    },
    "keywords": [
        "python"
    ],
    "categories": [
        "Programming Languages"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        },
        "virtualWorkspaces": true
    },
    "activationEvents": [
        "onLanguage:python"
    ],
    "main": "./out/extension",
    "scripts": {
        "compile": "tsc -watch -p ./",
        "build": "tsc -p ./",
        "package": "vsce package",
        "runTest": "node ./out/tests/standardTest.js"
    },
    "contributes": {
        "configuration": {
            "properties": {
                "python.pythonPath": {
                    "default": "python",
                    "description": "(DEPRECATED: Note this setting is not used when in pythonDeprecatePythonPath experiment) Path to Python, you can use a custom version of Python by modifying this setting to include the full path.",
                    "scope": "resource",
                    "type": "string"
                }
            }
        }
    },
    "dependencies": {
        "fs-extra": "^9.1.0"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/chai-arrays": "^1.0.2",
        "@types/fs-extra": "^9.0.11",
        "@types/glob": "^5.0.35",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.19.12",
        "@types/vscode": "^1.53.0",
        "@vscode/test-electron": "^1.6.1",
        "chai": "^4.1.2",
        "chai-arrays": "^2.0.0",
        "glob": "^7.1.2",
        "mocha": "^9.1.3",
        "mocha-junit-reporter": "^2.0.2",
        "mocha-multi-reporters": "^1.1.7",
        "tty": "^1.0.1",
        "tty-browserify": "0.0.1",
        "typescript": "^4.0.2",
        "typescript-formatter": "^7.1.0",
        "vsce": "^1.87.1"
    }
}
